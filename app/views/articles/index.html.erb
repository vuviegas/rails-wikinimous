<div class="container">
  <div class="navbar-header">
    <h1><%= link_to 'Wikinimous', articles_path %></h1>
    <h3><%= link_to 'New Article', new_article_path %></h3>
  </div>
  <% @articles.each do |article| %>
    <div class="title-list">
      <h2><%= link_to article.title.capitalize, article_path(article.id) %></h2>
      <p><%= article.content %></p>
    </div>
  <% end %>
</div>
